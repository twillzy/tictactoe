{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "ecmaVersion": 8,
      "jsx": true
    }
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".spec.js"
        ]
      }
    }
  },
  "globals": {
    "__DEV__": true
  },
  "env": {
    "es6": true,
    "jasmine": true,
    "jest": true,
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb",
    "plugin:import/errors"
  ],
  "rules": {
    "react/jsx-filename-extension": 0,
    "no-use-before-define": 0,
    "react/no-typos": 0,
    "arrow-body-style": [
      "off"
    ],
    "arrow-parens": [
      "off"
    ],
    "no-multi-assign": 0
  }
}
